<script type="text/javascript">

$(function() {

$("#menu_display input").click( function() {
	if ( $("#nav-pos").is(":hidden") ) {
		$("div#contentcell").animate({marginLeft: "200px"}, function() {
			$("#nav-pos").fadeIn(300);
		});		
		$(this).val("<< Menu");
		delCookie('no_menu')
	} else {
		$("#nav-pos").fadeOut(200, function() {
			$("div#contentcell").animate({marginLeft: "0px"});
		});
		$(this).val(">> Menu")
		setCookie('no_menu', 'yes')
	}
});


});

</script>
