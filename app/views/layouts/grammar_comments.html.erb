<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"  "http://www.w3.org/TR/html4/loose.dtd">
<html>
  <head>
  <title><%= @page_title %></title>
  <meta http-equiv="Content-Type" content="text/html; charset=utf8"> 

  <%= javascript_include_tag 'jquery.min.js' %>
  <%= javascript_include_tag 'effects.js' %>
  <%= stylesheet_link_tag 'stylesheet.css' %>
  <%= stylesheet_link_tag 'stylesounds.css' %>
  
  
  

<style type="text/css">
html {
height: auto ! important;
}

body {
height: auto !important;
min-height: 300px;
margin: 20px 30px 30px 30px;
background-color: #ffffff;
background-image: url("/images/shdw.gif");
background-position: left 0px;
background-repeat: repeat-x;
text-align: left;
}

div#contentcell {
border: 0;
margin: 0;
padding: 0;
background: none;
}

div#contfield {
background: none;
padding: 0;
}
.com-example {
padding-left: 30px;
}

</style>

<%= yield :special_js_css %>


</head>
<body>
  <div id="contentcell">
    <div id="contfield" class="content">
      <div style="display: no ne;">
        <div id="loading"><div id="progress"></div><div id="progressbar"><div id="progressStatus"></div></div></div>
      </div>
      <div id="mess" class="sound_tip"><i>Tip</i></div>
      <div id="audiospan" style="position: absolute;"></div>

      <div id="pagecontent">
        <div id="content">
		<!-- div style="position: fixed; background-color: #dfdfdf; width: 100%; left: 0; top: 0; padding: 3px 0; border: 1px solid #999;">&nbsp;&nbsp;&nbsp;&nbsp;<input type="button" style="font-size: 11px; font-weight: bold;" onclick="history.go(-1);" value="<< Back"/></div -->

		  <%= yield %>
        </div>
      </div>
    </div>
  </div>
  
  
<div id="analytic">
    <script type="text/javascript">
      var gaJsHost = (("https:" == document.location.protocol) ? "https://ssl." : "http://www.");
      document.write(unescape("%3Cscript src='" + gaJsHost + "google-analytics.com/ga.js' type='text/javascript'%3E%3C/script%3E"));
    </script>
    <script type="text/javascript">
      try {
        var pageTracker = _gat._getTracker("UA-8218265-1");
        pageTracker._trackPageview();
      } catch(err) {}</script>

</div>  
  
  
</body>
</html>













