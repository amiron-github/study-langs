<% @st_ex_completed = defined?(@st_ex_completed)?@st_ex_completed:'Exercises completed'; @st_ex_completed1 = defined?(@st_ex_completed1)?@st_ex_completed1:'Exercises completed'; @st_av_result = defined?(@st_av_result)?@st_av_result:'Average result'; @st_from = defined?(@st_from)?@st_from:'from'; @st_total_result = defined?(@st_total_result)?@st_total_result:'Total result'; @st_visit_les = defined?(@st_visit_les)?@st_visit_les:'Visit the page Â»'; @st_del_res = defined?(@st_del_res)?@st_del_res:'Delete results'; @st_expand =  defined?(@st_expand)?@st_expand:'Expand'%><li id="<%= @lesson_info[:html_id] %>"><h3>   <%= @lesson_info[:title] %>    <small title="<%= @st_ex_completed %>: <%= @lesson_info[:u_ex_len]%>">(<%= @lesson_info[:u_ex_len]%>)</small> 	<span class="cat-progress" title="<%= @lesson_info[:progress] %>%"><span style="width: <%= @lesson_info[:progress] %>%;"></span></span>   <span class="vu-li-index" title="<%= @st_expand %>">[+]</span></h3>		<div class="vu-inner"><div class="vu-u-w-len" ><div> <%= @lesson_info[:progress] %>%</div></div><table width="100%"><col><col width="100"><tr valign="top"><td><%= @st_ex_completed1 %>: <%= @lesson_info[:u_ex_len] %> <%= @st_from %> <%= @lesson_info[:n_ex_len] %><br/><%=@st_av_result%>: <%= @lesson_info[:ex_average]%>%<br/><br/><table class="v-e-r"><col width="170"><col><% @lesson_info[:tests].each do |test| %> <%if @ex_title_lang =="ru" then @t_ex_title = test[:data][:title_ru] elsif @ex_title_lang =="fr" then @t_ex_title = test[:data][:title_fr] else @t_ex_title = test[:data][:title] end%>  <tr><td><%= @t_ex_title %> </td><td align="center"><%= test[:result] %>%</td></tr><%end%></table><br/><span class="v-gen"><%= @st_total_result %>: <%= @lesson_info[:progress] %>%</td><td><a href="<%= @lesson_info[:lesson_url]%>" class="btn-like" ><%= @st_visit_les %></a>   <a href="javascript:;" <%= @lesson_info[:delete_func]%> class="btn-like" ><%= @st_del_res %></a></td></tr></table>   		</div></li>